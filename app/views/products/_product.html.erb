<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= product.product_type %>
  </p>

  <p>
    <strong>Protein:</strong>
    <%= product.protein %>
  </p>

  <p>
    <strong>Fat:</strong>
    <%= product.fat %>
  </p>

  <p>
    <strong>Carbohydrates:</strong>
    <%= product.carbohydrates %>
  </p>

  <p>
    <strong>Calories:</strong>
    <%= product.calories %>
  </p>

</div>
